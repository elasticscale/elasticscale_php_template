{
  "name": "Laravel Stateless Dev Environment",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/var/www/laravel",
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "xdebug.php-debug",
        "mikestead.dotenv"
      ]
    }
  },  
  "containerUser": "root",
  "remoteUser": "root",
  "features": {
    "git": "latest",
    "github-cli": "latest"
  }
}